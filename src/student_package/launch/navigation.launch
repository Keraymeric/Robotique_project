<launch>

  <!-- Run move_base -->
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find student_package)/config/move_base_params.yaml" command="load" />
    <rosparam file="$(find student_package)/config/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find student_package)/config/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find student_package)/config/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find student_package)/config/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find student_package)/config/dwa_local_planner_params.yaml" command="load" />
    <rosparam file="$(find student_package)/config/global_planner_params.yaml" command="load" />

    <param name="base_global_planner" value="navfn/NavfnROS" />
    <param name="base_local_planner" value="dwa_local_planner/DWAPlannerROS" />
    <param name="controller_frequency" value="5.0" />
    <param name="controller_patience" value="15.0" />
  </node>

</launch>